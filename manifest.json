{
	"manifest_version": 2,
	"name": "WhatsApp Web Hide Media",
	"description": "Hide media, hover to see it",
	"version": "1.0",
	"permissions": ["https://web.whatsapp.com/"],
  "browser_action": {
    "default_icon": {
        "19": "icon19x19.png",
        "38": "icon38x38.png"
    }
	},
	"content_scripts": [
		{
			"matches": [
				"https://web.whatsapp.com/*"
			],
			"css": ["style.css"]
		}
	]
}
